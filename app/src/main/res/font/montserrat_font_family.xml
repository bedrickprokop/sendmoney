<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!-- normal -->
    <font
        android:font="@font/montserrat"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/montserrat"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!-- medium -->
    <font
        android:font="@font/montserrat_medium"
        android:fontStyle="normal"
        android:fontWeight="550"
        app:font="@font/montserrat_medium"
        app:fontStyle="normal"
        app:fontWeight="550" />

    <!-- bold -->
    <font
        android:font="@font/montserrat_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/montserrat_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />

</font-family>